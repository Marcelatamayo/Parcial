<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.marcela.parcial2">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name=".Login">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".GestionVentas"
            android:label="Gestión Ventas"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".GestionClientes"
            android:label="Gestión Clientes"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".NuevaVenta"
            android:label="Nueva Venta"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".ConsultarVenta"
            android:label="Consultar Venta"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".DetalleVenta"
            android:label="Detalle Venta"
            android:theme="@style/AppTheme.NoActionBar" />
    </application>

</manifest>